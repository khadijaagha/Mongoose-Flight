<%- include('../partials/header')%>
<h2>All Flights</h2>
<table>
  <thead>
    <tr>
      <th>Airline</th>
      <th>Airport</th>
      <th>Flight Number</th>
      <th>Departure</th>
      <th></th>
      
    </tr>
  </thead>
  <tbody>
    
    <% flights.forEach(function(f) { %>
        <tr>
            
            <td><%= f.airline %></td>
            <td><%= f.airport %></td>
            <td><a href="/flights/<%= f._id %>"><%= f.flightNo %></a></td>
            <td><%= f.departs %></td>
            <td><a href="/flights/<%= f._id %>">Details</a></td>
          
        </tr>
      
    <% }) %>



<%- include('../partials/footer')%>